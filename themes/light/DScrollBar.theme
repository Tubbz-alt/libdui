DUI--DScrollBar::add-line,DUI--DScrollBar::sub-line,DUI--DScrollBar::add-page,DUI--DScrollBar::sub-page{
    background-color: transparent;
}

DUI--DScrollBar{
    max-width: 10;
    margin-right: 1px;
    background-color: transparent;
}

DUI--DScrollBar::handle{
    background-color: #33000000;
    border: 0.5px solid #11FFFFFF;
    border-radius: 3px;
}
