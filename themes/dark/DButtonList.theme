DUI--DButtonList {
    background-color: grey;
}

DUI--DButtonList QFrame#BorderFrame{
  background-color: transparent;
}

DUI--DButtonList QPushButton{
    background-color: transparent;
    color: white;
    border: none;
    border-radius:3px;
    text-align: left center;
    padding-left: 40;
}

DUI--DButtonList QPushButton:checked{
    background-color: lightblack;
}

DUI--DButtonList QPushButton:checked[state="OnlyOne"]{
    background-color: lightblack;
    color:#01BDFF;
}

DUI--DButtonList QPushButton:checked[state="normal"]{
    background-color: transparent;
    color:#01BDFF;
}
